{
  "name": "new-project",
  "version": "1.0.0",
  "description": "Default project",
  "main": "index.js",
  "scripts": {
    
    "build:js": "uglifyjs assets/scripts/custom.js -c -m -o assets/scripts/custom.min.js",
    "build:js2folder": "uglifyjs-folder assets/scripts/inputfolder -eo assets/scripts/outputfolder",

    "compile:sass": "node-sass assets/styles/custom.scss assets/styles/custom.css",
    "compile:css": "sass assets/styles/custom.scss:assets/styles/custom.css --style expanded",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" assets/styles/custom.css -o assets/styles/custom.prefixed.css",
    "compress:css": "node-sass assets/styles/custom.prefixed.css assets/styles/custom.min.css --output-style compressed",
    "run:localhost": "nodemon index.js",
    "build:process": "npm-run-all build:js compile:css prefix:css compress:css run:localhost"

  },
  "author": "Muhammad Javed Khalil",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.5",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.1",
    "uglify-js": "^3.4.9",
    "uglifyjs-folder": "^1.5.1"
  }
}
